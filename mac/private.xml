<?xml version="1.0"?>
<root>
	<symbol_map type="KeyCode" name="[" value="0x1e" />
	<appdef>
		<appname>iTERM2</appname>
		<equal>com.googlecode.iterm2</equal>
	</appdef>
	<appdef>
		<appname>MACVIM</appname>
		<equal>org.vim.MacVim</equal>
	</appdef>
	<appdef>
		<appname>PhpStorm</appname>
		<equal>com.jetbrains.PhpStorm</equal>
	</appdef>
	<appdef>
		<appname>Atom</appname>
		<equal>com.github.atom</equal>
	</appdef>
	<appdef>
		<appname>SublimeText3</appname>
		<equal>com.sublimetext.3</equal>
	</appdef>
	<list>
		<item>
			<name>Leave InsertMode With Eisuu For Vim</name>
			<identifier>remap.wanashi.vimEscWithEisuu</identifier>
			<only>TERMINAL,iTERM2,MACVIM,PhpStorm,Atom,SublimeText3</only>
			<autogen>--KeyToKey-- KeyCode::ESCAPE, KeyCode::ESCAPE, KeyCode::JIS_EISUU</autogen>
			<autogen>--KeyToKey-- KeyCode::[, VK_CONTROL, KeyCode::C, VK_CONTROL, KeyCode::JIS_EISUU</autogen>
			<autogen>--KeyToKey-- KeyCode::C, VK_CONTROL, KeyCode::C, VK_CONTROL, KeyCode::JIS_EISUU</autogen>
		</item>
		<item>
			<name>Disable Cmd + Q</name>
			<identifier>remap.wanashi.disableCmdQ</identifier>
			<only>GOOGLE_CHROME, TERMINAL, SKYPE</only>
			<autogen>__KeyToKey__ KeyCode::Q, VK_COMMAND | ModifierFlag::NONE, KeyCode::VK_NONE</autogen>
			<autogen>__KeyToKey__ KeyCode::Q, VK_COMMAND | VK_SHIFT | ModifierFlag::NONE, KeyCode::Q, ModifierFlag::COMMAND_L</autogen>
		</item>
		<item>
			<name>Disable Cmd + W</name>
			<identifier>remap.wanashi.disableCmdW</identifier>
			<only>TERMINAL</only>
			<autogen>__KeyToKey__ KeyCode::W, VK_COMMAND | ModifierFlag::NONE, KeyCode::VK_NONE</autogen>
			<autogen>__KeyToKey__ KeyCode::W, VK_COMMAND | VK_SHIFT | ModifierFlag::NONE, KeyCode::W, ModifierFlag::COMMAND_L</autogen>
		</item>
		<item>
			<name>Disable Cmd + P</name>
			<identifier>remap.wanashi.disableCmdP</identifier>
			<only>GOOGLE_CHROME, TERMINAL, SKYPE</only>
			<autogen>__KeyToKey__ KeyCode::P, VK_COMMAND | ModifierFlag::NONE, KeyCode::VK_NONE</autogen>
			<autogen>__KeyToKey__ KeyCode::P, VK_COMMAND | VK_SHIFT | ModifierFlag::NONE, KeyCode::P, ModifierFlag::COMMAND_L</autogen>
		</item>
		<item>
			<name>Disable Cmd + D</name>
			<identifier>remap.wanashi.disableCmdD</identifier>
			<only>TERMINAL</only>
			<autogen>__KeyToKey__ KeyCode::D, VK_COMMAND | ModifierFlag::NONE, KeyCode::VK_NONE</autogen>
			<autogen>__KeyToKey__ KeyCode::D, VK_COMMAND | VK_SHIFT | ModifierFlag::NONE, KeyCode::D, ModifierFlag::COMMAND_L</autogen>
		</item>
		<item>
			<name>Disable Cmd + T</name>
			<identifier>remap.wanashi.disableCmdT</identifier>
			<only>TERMINAL</only>
			<autogen>__KeyToKey__ KeyCode::T, VK_COMMAND | ModifierFlag::NONE, KeyCode::VK_NONE</autogen>
			<autogen>__KeyToKey__ KeyCode::T, VK_COMMAND | VK_SHIFT | ModifierFlag::NONE, KeyCode::T, ModifierFlag::COMMAND_L</autogen>
		</item>
		<item>
			<name>Move Vim KeyBind With Eisu</name>
			<identifier>remap.wanashi.eisuVimBind</identifier>
			<autogen>__KeyOverlaidModifier__ KeyCode::JIS_EISUU, KeyCode::VK_MODIFIER_EXTRA1, KeyCode::JIS_EISUU</autogen>
			<autogen>__KeyToKey__ KeyCode::H, ModifierFlag::EXTRA1, KeyCode::CURSOR_LEFT</autogen>
			<autogen>__KeyToKey__ KeyCode::J, ModifierFlag::EXTRA1, KeyCode::CURSOR_DOWN</autogen>
			<autogen>__KeyToKey__ KeyCode::K, ModifierFlag::EXTRA1, KeyCode::CURSOR_UP</autogen>
			<autogen>__KeyToKey__ KeyCode::L, ModifierFlag::EXTRA1, KeyCode::CURSOR_RIGHT</autogen>
		</item>
		<item>
			<name>Map F19 To Hyper</name>
			<identifier>remap.wanashi.f19ToHyper</identifier>
			<autogen>--KeyToKey-- KeyCode::F19, KeyCode::COMMAND_L, ModifierFlag::OPTION_L | ModifierFlag::SHIFT_L | ModifierFlag::CONTROL_L</autogen>
		</item>
		<item>
			<name>Disable Cmd + Tab</name>
			<identifier>remap.wanashi.disableCmdTab</identifier>
			<autogen>__KeyToKey__ KeyCode::TAB, VK_COMMAND | ModifierFlag::NONE, KeyCode::VK_NONE</autogen>
			<autogen>__KeyToKey__ KeyCode::TAB, VK_COMMAND | VK_SHIFT | ModifierFlag::NONE, KeyCode::VK_NONE</autogen>
		</item>
	</list>
</root>
