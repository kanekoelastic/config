{
  "title": "Don't quit App when pressing LCommand + q",
  "rules": [
    {
      "description": "Don't quit App when pressing LCommand + q",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "q"
            }
          ],
          "conditions": [
            {
              "bundle_identifiers": [
                "^com.apple.Terminal$",
                "^com.evernote.Evernote$",
                "^com.google.Chrome$",
                "^com.zive.kiwi$",
                "^com.tinyspeck.slackmacgap$"
              ],
              "type": "frontmost_application_if"
            }
          ]
        }
      ]
    }
  ]
}
